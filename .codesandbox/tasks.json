{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true
    },
    "development": {
      "name": "development",
      "command": "npm run development",
      "runAtStart": false
    },
    "watch": {
      "name": "watch",
      "command": "npm run watch",
      "runAtStart": false
    },
    "watch-poll": {
      "name": "watch-poll",
      "command": "npm run watch-poll",
      "runAtStart": false
    },
    "hot": {
      "name": "hot",
      "command": "npm run hot",
      "runAtStart": false
    },
    "prod": {
      "name": "prod",
      "command": "npm run prod",
      "runAtStart": false
    },
    "production": {
      "name": "production",
      "command": "npm run production",
      "runAtStart": false
    },
    "npx browserslist@latest --update-db": {
      "name": "npx browserslist@latest --update-db",
      "command": "npx browserslist@latest --update-db"
    },
    "stats.children: true' resp. '--stats-children' ": {
      "name": "stats.children: true' resp. '--stats-children' ",
      "command": "stats.children: true' resp. '--stats-children' "
    }
